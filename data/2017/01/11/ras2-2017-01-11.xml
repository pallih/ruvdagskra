    <event event-id="4879386" serie-id="23825" start-time="2017-01-11 16:05:00" duration="01:55:00" mark="no">
      <title>Síðdegisútvarpið</title>
      <original-title />
      <description><![CDATA[Umsjón: Guðmundur Pálsson og Björg Magnúsdóttir.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
