    <event event-id="4870110" serie-id="23674" start-time="2017-10-03 12:20:00" duration="00:22:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 03. október 2017</original-title>
      <description />
