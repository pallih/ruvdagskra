    <event event-id="4869745" serie-id="23674" start-time="2017-05-09 12:20:00" duration="00:22:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 9. maí 2017</original-title>
      <description />
      <short-description />
