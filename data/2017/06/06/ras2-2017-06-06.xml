    <event event-id="4881891" serie-id="23842" start-time="2017-06-06 22:05:00" duration="01:55:00" mark="no">
      <title>Rokkland</title>
      <original-title>Chris Cornell seinni hluti +</original-title>
      <description><![CDATA[Umsjón: Ólafur Páll Gunnarsson.
(Frá því á sunnudag)]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>yes</rerun>
