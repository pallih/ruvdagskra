    <event event-id="4885189" serie-id="23863" start-time="2017-11-12 12:40:00" duration="02:15:00" mark="no">
      <title>Sunnudagssögur</title>
      <original-title>Rúna Einarsdóttir hestakona og Fabúla tónlistarkona</original-title>
      <description><![CDATA[Umsjón: Hrafnhildur Halldórsdóttir.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
