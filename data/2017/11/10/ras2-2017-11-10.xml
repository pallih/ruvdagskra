    <event event-id="4870138" serie-id="23674" start-time="2017-11-10 12:20:00" duration="00:22:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 10. nóvember 2017</original-title>
      <description />
