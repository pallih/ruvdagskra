    <event event-id="4879455" serie-id="23825" start-time="2017-04-24 16:05:00" duration="01:55:00" mark="no">
      <title>Síðdegisútvarpið</title>
      <original-title>Síðdegisútvarpið 24. Apríl 2017</original-title>
      <description><![CDATA[Umsjón: Guðmundur Pálsson og Björg Magnúsdóttir.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
