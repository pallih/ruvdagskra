    <event event-id="4869987" serie-id="23674" start-time="2017-04-13 12:20:00" duration="00:17:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 13 apríl 2017</original-title>
      <description />
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
