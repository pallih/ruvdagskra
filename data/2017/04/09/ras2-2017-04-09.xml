    <event event-id="4885210" serie-id="23864" start-time="2017-04-09 15:02:00" duration="00:55:17" mark="no">
      <title>Talandi um það</title>
      <original-title>Kristján Ingi og Einar Bogi</original-title>
      <description><![CDATA[Umsjón: Andri Freyr Viðarsson.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
