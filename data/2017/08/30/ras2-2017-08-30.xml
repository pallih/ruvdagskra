    <event event-id="4896542" serie-id="23822" start-time="2017-08-30 06:50:00" duration="02:10:00" mark="no">
      <title>Morgunútvarpið</title>
      <original-title>Morgunútvarpið 30. Ágúst 2017</original-title>
      <description><![CDATA[Umsjón: Guðrún Sóley Gestsdóttir og Aðalsteinn Kjartansson.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
