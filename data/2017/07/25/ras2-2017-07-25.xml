    <event event-id="4870060" serie-id="23674" start-time="2017-07-25 12:20:00" duration="00:22:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegifréttir25.07.2017</original-title>
      <description />
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
      <subtitles>
        <open>no</open>
        <closed>no</closed>
