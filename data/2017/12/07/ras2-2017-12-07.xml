    <event event-id="4879297" serie-id="23824" start-time="2017-12-07 12:45:00" duration="03:15:00" mark="no">
      <title>Dagvaktin</title>
      <original-title>Tappi tíkarrass, dagur íslenskrar tónlistar og jólalagakeppnin.</original-title>
      <description><![CDATA[Umsjón: Hulda Geirsdóttir.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
