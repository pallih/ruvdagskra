    <event event-id="4886004" serie-id="23874" start-time="2017-09-10 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 9. septemer 2017</original-title>
      <description />
      <short-description />
      <rating />
