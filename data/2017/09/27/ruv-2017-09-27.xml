      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
      <aspect-ratio>
        <size>16:9</size>
        <anamorphic>yes</anamorphic>
        <letterbox>no</letterbox>
      </aspect-ratio>
      <subtitles>
        <open>no</open>
