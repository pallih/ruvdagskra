    <event event-id="4878687" serie-id="23822" start-time="2017-03-30 06:50:00" duration="03:10:00" mark="no">
      <title>Morgunútvarpið</title>
      <original-title>HB Grandi frestar því að hætta, Blekking Ólafs Ólafssonar</original-title>
      <description><![CDATA[Umsjón: Guðrún Sóley Gestsdóttir, Sigmar Guðmundsson og Aðalsteinn Kjartansson.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
