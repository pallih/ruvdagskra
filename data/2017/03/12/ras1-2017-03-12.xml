    <event event-id="4885848" serie-id="23874" start-time="2017-03-12 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 12. mars 2017</original-title>
      <description />
      <short-description />
