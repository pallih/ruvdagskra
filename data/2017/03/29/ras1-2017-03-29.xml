    <event event-id="4869716" serie-id="23674" start-time="2017-03-29 12:20:00" duration="00:22:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 29. mars 2017</original-title>
      <description />
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
