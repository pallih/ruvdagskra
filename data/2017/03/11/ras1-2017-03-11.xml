    <event event-id="4885847" serie-id="23874" start-time="2017-03-11 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 11. mars 2017</original-title>
      <description />
