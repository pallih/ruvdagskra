    <event event-id="4878650" serie-id="23822" start-time="2017-02-07 06:50:00" duration="03:10:00" mark="no">
      <title>Morgunútvarpið</title>
      <original-title>Tattó, Öryggismyndavélar, Geimurinn, Svíar og sprell</original-title>
      <description><![CDATA[Umsjón: Guðrún Sóley Gestsdóttir, Sigmar Guðmundsson og Aðalsteinn Kjartansson.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
