    <event event-id="4885944" serie-id="23874" start-time="2017-02-12 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 12. febrúar 2016</original-title>
      <description />
