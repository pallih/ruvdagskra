    <event event-id="4869937" serie-id="23674" start-time="2017-02-02 12:20:00" duration="00:22:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>02.02.2017 Hádegisfréttir</original-title>
      <description />
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
