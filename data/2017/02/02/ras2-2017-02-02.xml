    <event event-id="4878647" serie-id="23822" start-time="2017-02-02 07:03:00" duration="02:57:00" mark="no">
      <title>Morgunútvarpið</title>
      <original-title>Benedikt Jóhannesson um svarta hagkerfið, áhrif Trump á ísland</original-title>
      <description><![CDATA[Umsjón: Guðrún Sóley Gestsdóttir, Sigmar Guðmundsson og Aðalsteinn Kjartansson.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
