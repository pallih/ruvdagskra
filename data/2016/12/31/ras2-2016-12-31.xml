    <event event-id="4841914" serie-id="21624" start-time="2016-12-31 09:05:00" duration="03:15:00" mark="no">
      <title>Bergsson og Blöndal</title>
      <original-title>Bergsson og Blöndal- Karnival og kveðjustund</original-title>
      <description><![CDATA[Felix Bergsson og Margrét Blöndal taka á móti gestum og kveðja hlustendur að sinni.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
