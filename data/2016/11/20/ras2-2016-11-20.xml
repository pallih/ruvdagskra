    <event event-id="4853970" serie-id="23353" start-time="2016-11-20 15:03:00" duration="00:57:00" mark="no">
      <title>Talandi um það</title>
      <original-title>Sigrún Arnardóttir</original-title>
      <description><![CDATA[Umsjón: Andri Freyr Viðarsson.]]></description>
      <short-description />
