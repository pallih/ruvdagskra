    <event event-id="4840901" serie-id="21590" start-time="2016-11-23 07:03:00" duration="02:55:00" mark="no">
      <title>Morgunútvarpið</title>
      <original-title>Valgerður frá SÁÁ, Stjórnarmyndunarviðræður, Ari Edwald</original-title>
      <description><![CDATA[Umsjón: Guðrún Sóley Gestsdóttir, Sigmar Guðmundsson og Aðalsteinn Kjartansson.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
