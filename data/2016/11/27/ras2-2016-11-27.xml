    <event event-id="4853971" serie-id="23353" start-time="2016-11-27 15:03:00" duration="00:57:00" mark="no">
      <title>Talandi um það</title>
      <original-title>Hálfdán Pedersen</original-title>
      <description><![CDATA[Umsjón: Andri Freyr Viðarsson.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
