    <event event-id="4853410" serie-id="23339" start-time="2016-11-26 10:15:00" duration="00:40:00" mark="no">
      <title>Þúsund ferðamenn á færibandi</title>
      <original-title>Ferðamönnum fjölgar ört</original-title>
      <description><![CDATA[Umsjón: Dagur Gunnarsson.
(Aftur á morgun)]]></description>
      <short-description />
      <rating />
      <episode number="3" number-of-episodes="4" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
