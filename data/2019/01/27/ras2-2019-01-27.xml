    <event event-id="4984152" serie-id="25243" start-time="2019-01-27 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir sunnudaginn 27. janúar 2019</original-title>
      <description />
