    <event event-id="4984145" serie-id="25243" start-time="2019-01-20 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 20. janúar 2019</original-title>
      <description />
