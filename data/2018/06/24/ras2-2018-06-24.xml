    <event event-id="4923441" serie-id="25301" start-time="2018-06-24 12:40:00" duration="02:15:00" mark="no">
      <title>Sunnudagssögur</title>
      <original-title>Baldur Þórir Guðmundsson og Edda Björgvins</original-title>
      <description><![CDATA[Umsjón: Hrafnhildur Halldórsdóttir.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
