    <event event-id="4919881" serie-id="25279" start-time="2018-06-26 16:05:00" duration="01:20:00" mark="no">
      <title>Síðdegisútvarpið</title>
      <original-title>26. júní</original-title>
      <description><![CDATA[Umsjón: Guðmundur Pálsson, Hafdís Helga Helgadóttir og Andri Freyr Viðarsson.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
      <subtitles>
        <open>no</open>
        <closed>no</closed>
