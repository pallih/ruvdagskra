    <event event-id="4909207" serie-id="25243" start-time="2018-06-26 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 26. júní 2018</original-title>
      <description />
