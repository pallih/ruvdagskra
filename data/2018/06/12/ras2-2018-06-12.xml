    <event event-id="4919872" serie-id="25279" start-time="2018-06-12 16:05:00" duration="02:30:00" mark="no">
      <title>Síðdegisútvarpið</title>
      <original-title>12. júní</original-title>
      <description><![CDATA[Umsjón: Hafdís Helga Helgadóttir og Andri Freyr Viðarsson.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
