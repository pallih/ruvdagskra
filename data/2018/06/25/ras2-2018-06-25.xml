    <event event-id="4915771" serie-id="25257" start-time="2018-06-25 06:50:00" duration="02:00:00" mark="no">
      <title>Morgunútvarpið</title>
      <original-title>Landakaup útlendinga, kattadráp, Hvalárvirkjun, íþróttir og málfar</original-title>
      <description><![CDATA[Umsjón: Sigmar Guðmundsson og Hulda G. Geirsdóttir.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
