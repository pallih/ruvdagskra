    <event event-id="4923438" serie-id="25301" start-time="2018-06-03 12:40:00" duration="02:15:00" mark="no">
      <title>Sunnudagssögur</title>
      <original-title>Kristín Linda og lífsviðhorfin og Einar Scheving og tónlistin</original-title>
      <description><![CDATA[Umsjón: Hrafnhildur Halldórsdóttir.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
