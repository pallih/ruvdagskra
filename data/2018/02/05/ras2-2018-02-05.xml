    <event event-id="4909066" serie-id="25243" start-time="2018-02-05 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 05.02.2018</original-title>
      <description />
