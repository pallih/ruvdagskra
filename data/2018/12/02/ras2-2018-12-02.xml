    <event event-id="4909366" serie-id="25243" start-time="2018-12-02 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir sunnudaginn 2. desember 2018</original-title>
      <description />
