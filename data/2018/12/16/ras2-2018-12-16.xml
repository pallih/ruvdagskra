    <event event-id="4923465" serie-id="25301" start-time="2018-12-16 12:40:00" duration="02:15:00" mark="no">
      <title>Sunnudagssögur</title>
      <original-title>Ásdís Halla og Einar Mantyla</original-title>
      <description><![CDATA[Umsjón: Hrafnhildur Halldórsdóttir.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <offair>no</offair>
      <header>no</header>
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
