      <title>Konsert</title>
      <original-title>Jóla Eivør í Silfurbergi</original-title>
      <description><![CDATA[Umsjón: Ólafur Páll Gunnarsson.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <offair>no</offair>
      <header>no</header>
      <subevent>no</subevent>
