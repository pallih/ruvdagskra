    <event event-id="4909152" serie-id="25243" start-time="2018-05-02 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 02.maí 2018</original-title>
      <description />
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
