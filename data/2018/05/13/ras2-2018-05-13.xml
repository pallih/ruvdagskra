    <event event-id="4923154" serie-id="25296" start-time="2018-05-13 18:00:00" duration="00:10:00" mark="no">
      <title>Kvöldfréttir</title>
      <original-title>Kvöldfréttir 13. maí 2018</original-title>
      <description />
      <short-description />
