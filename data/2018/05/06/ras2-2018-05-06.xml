    <event event-id="4923540" serie-id="25303" start-time="2018-05-06 16:05:00" duration="01:55:00" mark="no">
      <title>Rokkland</title>
      <original-title>Bassi, trommur og raddir duga.. í bili.</original-title>
      <description><![CDATA[Umsjón: Ólafur Páll Gunnarsson.
(Aftur á þriðjudag)]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
      <subtitles>
        <open>no</open>
