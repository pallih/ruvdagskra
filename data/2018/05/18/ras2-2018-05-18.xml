    <event event-id="4924216" serie-id="25313" start-time="2018-05-18 19:23:00" duration="02:35:00" mark="no">
      <title>Füzz</title>
      <original-title>Birgitta - Purple og Oasis</original-title>
      <description><![CDATA[Umsjón: Ólafur Páll Gunnarsson.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
