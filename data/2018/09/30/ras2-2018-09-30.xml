    <event event-id="4923454" serie-id="25301" start-time="2018-09-30 12:40:00" duration="02:15:00" mark="no">
      <title>Sunnudagssögur</title>
      <original-title>Sigrún Waage leikkona og Hallfríður Ólafsdóttir flautuleikari</original-title>
      <description><![CDATA[Umsjón: Hrafnhildur Halldórsdóttir.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
