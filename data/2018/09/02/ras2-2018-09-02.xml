    <event event-id="4975348" serie-id="25816" start-time="2018-09-02 15:02:00" duration="00:55:00" mark="no">
      <title>Tímaflakk með Bergsson og Blöndal</title>
      <original-title>Tímaflakkið 2. september 2018</original-title>
      <description><![CDATA[Felix Bergsson og Margrét Blöndal fara með hlustendur í tímaflakk.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
