      <original-title>Spegillinn 10. september 2018. Aðgerðaáætlun ríkisstjórnarinnar í loft</original-title>
      <description />
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
