      <original-title>Spegillinn 26. september 2018. Salan á Bakkavör, nýsköpun eftir hrun,</original-title>
      <description />
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
