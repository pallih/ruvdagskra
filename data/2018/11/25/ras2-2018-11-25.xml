    <event event-id="4909359" serie-id="25243" start-time="2018-11-25 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 25. Nóvember</original-title>
      <description />
