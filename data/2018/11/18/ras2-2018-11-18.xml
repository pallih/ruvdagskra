    <event event-id="4909352" serie-id="25243" start-time="2018-11-18 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 18. nóvember 2018</original-title>
      <description />
