    <event event-id="4923456" serie-id="25301" start-time="2018-10-14 12:40:00" duration="02:15:00" mark="no">
      <title>Sunnudagssögur</title>
      <original-title>Jóhann Vilhjálmsson og Stella Samúelsdóttir</original-title>
      <description><![CDATA[Umsjón: Hrafnhildur Halldórsdóttir.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
