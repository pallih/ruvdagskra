    <event event-id="4909324" serie-id="25243" start-time="2018-10-21 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir sunnudaginn 21. október 2018</original-title>
      <description />
      <short-description />
      <rating />
