      <original-title>Spegillinn 21.ágúst 2018. Viðbrögð við gjaldþrotum flugfélaga, gæðamat</original-title>
      <description />
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
