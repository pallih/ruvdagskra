    <event event-id="4955499" serie-id="25299" start-time="2018-08-18 22:05:00" duration="02:30:00" mark="no">
      <title>Næturvaktin</title>
      <original-title>Menning og næturvakt</original-title>
      <description><![CDATA[Umsjón: Heiða Eiríksdóttir.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
