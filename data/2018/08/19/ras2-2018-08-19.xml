    <event event-id="4923448" serie-id="25301" start-time="2018-08-19 12:40:00" duration="02:15:00" mark="no">
      <title>Sunnudagssögur</title>
      <original-title>Ingibjörg Valgeirsdóttir og Guðmundur Steingrímsdóttir</original-title>
      <description><![CDATA[Umsjón: Helga Arnardóttir.]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
