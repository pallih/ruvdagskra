    <event event-id="4945229" serie-id="25296" start-time="2018-08-19 18:00:00" duration="00:10:00" mark="no">
      <title>Kvöldfréttir</title>
      <original-title>Kvöldfréttir 19. ágúst 2018</original-title>
      <description />
      <short-description />
      <rating />
