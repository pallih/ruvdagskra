    <event event-id="4909237" serie-id="25243" start-time="2018-07-26 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 26.júlí 2018</original-title>
      <description />
