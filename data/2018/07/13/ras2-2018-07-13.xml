    <event event-id="4923186" serie-id="25296" start-time="2018-07-13 18:00:00" duration="00:10:00" mark="no">
      <title>Kvöldfréttir</title>
      <original-title>Kvöldfréttir 13. júlí 2018</original-title>
      <description />
      <short-description />
      <rating />
