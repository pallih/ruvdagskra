    <event event-id="4909227" serie-id="25243" start-time="2018-07-16 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 16. Júlí 2018</original-title>
      <description />
