    <event event-id="4923176" serie-id="25296" start-time="2018-07-03 18:00:00" duration="00:10:00" mark="no">
      <title>Kvöldfréttir</title>
      <original-title>Kvöldfréttir 3. júlí 2018</original-title>
      <description />
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
