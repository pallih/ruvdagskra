    <event event-id="4909044" serie-id="25243" start-time="2018-01-14 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 14. janúar 2018</original-title>
      <description />
