    <event event-id="4923524" serie-id="25303" start-time="2018-01-14 16:05:00" duration="01:43:50" mark="no">
      <title>Rokkland</title>
      <original-title>Iron &amp; Wine</original-title>
      <description><![CDATA[Umsjón: Ólafur Páll Gunnarsson.
(Aftur á þriðjudag)]]></description>
      <short-description />
      <rating />
      <episode number="1" number-of-episodes="1" multiple-episodes="yes" />
      <subevent>no</subevent>
      <live>no</live>
      <rerun>no</rerun>
      <subtitles>
