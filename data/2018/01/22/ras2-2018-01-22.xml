    <event event-id="4909052" serie-id="25243" start-time="2018-01-22 12:20:00" duration="00:20:00" mark="no">
      <title>Hádegisfréttir</title>
      <original-title>Hádegisfréttir 22.janúar 2018</original-title>
      <description />
